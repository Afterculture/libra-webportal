    <div class="col-md-4 col-sm-12 col-xs-12">
    {{#if char.profile_image}}
            {{#link-to 'file' (query-params path=char.profile_image)}}
                <img src="/uploads/{{char.profile_image}}" class="profile-image"/>
            {{/link-to}}
    {{else}}
        <img src="/uploads/theme_images/noicon.png" class="profile-image" />
    {{/if}}
    </div>

    <div class="col-md-8 col-sm-12 col-xs-12">        

        <div class="profile-box">
        <div class="profile-title">
                    {{char.military_name}}
        </div>
        
        {{#each char.demographics as |demo|}}
        <div class="row">
            <div class="col-sm-3">
                    <b>{{demo.name}}:</b> 
            </div>
            <div class="col-sm-9">
                    {{demo.value}}
            </div>
        </div>
        {{/each}}
                
        
        <div class="profile-divider">
        </div>
        
        
        {{#each char.groups as |group|}}
        <div class="row">
            <div class="col-sm-3">
                    <b>{{group.name}}:</b> 
            </div>
            <div class="col-sm-9">
                    {{group.value}}
            </div>
        </div>
        {{/each}}


        {{#if char.handle}}
        
        <div class="row">
            <div class="col-sm-3">
                    <b>Player:</b> 
            </div>
            <div class="col-sm-9">
                    <a href="http://arescentral.aresmush.com/handle/{{char.handle}}">{{char.handle}}</a>
            </div>
        </div>
        
        {{/if}}
        </div>
        
    </div>
