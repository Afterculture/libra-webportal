<div class="panel panel-info">
{{#if char.profile_gallery}}
    {{#each char.profile_gallery as |g|}}
        {{#link-to 'file' (query-params path=g.path)}}
        <img src="/uploads/{{g.path}}" class="profile-gallery">
        {{/link-to}}
    {{/each}}
{{/if}}
<div class="clearfix"></div>
</div>
